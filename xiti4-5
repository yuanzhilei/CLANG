#include <stdio.h>
#include <stdlib.h>

int main()
{
    int score;
    char grade;
    printf("enter score:");
    scanf("%d", &score);
    if (score>=90)
        grade='A';
    else if (score>=80&&score<=89)
        grade='B';
    else if (score>=70&&score<=79)
        grade='C';
    else if (score>=60&&score<=69)
        grade='D';
    else if (score<60)
        grade='E';
    else
        printf("score error!\n");
    printf("grade=%c\n",grade);
    return 0;
}
