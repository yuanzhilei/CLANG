#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c,max;
    printf("enter a,b,c:");
    scanf("%d,%d,%d", &a,&b,&c);
    if (a>b&&a>c)
        max=a;
    else if (b>a&&b>c)
        max=b;
    else if (c>a&&c>b)
        max=c;
    else
        printf("error!\n");
    printf("a=%d,b=%d,c=%d,max=%d\n",a,b,c,max);
    return 0;
}
